{
    "require": {
        "andreyryabin/sprint.migration": "dev-master",
        "webonyx/graphql-php": "^15.13",
        "bronevik/hotels-connector": "^5.2"
    },
    "autoload": {
        "psr-4": {"Local\\" : "lib/"}
    },
    "extra": {
        "installer-paths": {
            "../modules/{$name}/": ["type:bitrix-d7-module", "type:bitrix-module"],
            "../components/{$name}/": ["type:bitrix-d7-component", "type:bitrix-component"],
            "../templates/{$name}/": ["type:bitrix-d7-template", "type:bitrix-theme"]
        }
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "kylekatarnls/update-helper": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [{
            "type": "composer",
            "url": "https://gitlab.addamant-work.ru/api/v4/group/72/-/packages/composer/"
        }]
}
